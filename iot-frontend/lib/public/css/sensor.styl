#sensors, #sensor
  min-height 100%
  padding-bottom 60px
  .new-sensor-container
    position fixed
    z-index 10
    width 90%
    left 5%
    top 30px

  .background-area
    position absolute
    z-index 9
    top 0
    left 0
    width 100%
    height 100%
    background rgba(240,240,240,0.7)

  .colording
    background-color #E83A25
    content ''
    width 10px

  .blue
    stroke #00f
    fill #00f
  .red
    stroke #f00
    fill #f00
  .green
    stroke #0f0
    fill #0f0
    /*
     * d3
     */
    path {
      stroke-width: 2;
      fill: none;
    }

    .axis path,
    .axis line
      fill none
      stroke #000
      shape-rendering crispEdges

    .y.axis path
      display none

    .y.axis line
      stroke #777
      stroke-dasharray 2,2
